op){if(h.animating)return!1;h.fixLoop();h.container[0].clientLeft;return h.slideTo(h.activeIndex-1,e,t,n)}return h.slideTo(h.activeIndex-1,e,t,n)},h._slidePrev=function(t){return h.slidePrev(!0,t,!0)},h.slideReset=function(t,e,n){return h.slideTo(h.activeIndex,e,t)},h.disableTouchControl=function(){return h.params.onlyExternal=!0,!0},h.enableTouchControl=function(){return h.params.onlyExternal=!1,!0},h.setWrapperTransition=function(t,e){h.wrapper.transition(t),\"slide\"!==h.params.effect&&h.effects[h.params.effect]&&h.effects[h.params.effect].setTransition(t),h.params.parallax&&h.parallax&&h.parallax.setTransition(t),h.params.scrollbar&&h.scrollbar&&h.scrollbar.setTransition(t),h.params.control&&h.controller&&h.controller.setTransition(t,e),h.emit(\"onSetTransition\",h,t)},h.setWrapperTranslate=function(t,e,n){var i,r=0,o=0,a=0;h.isHorizontal()?r=h.rtl?-t:t:o=t,h.params.roundLengths&&(r=O(r),o=O(o)),h.params.virtualTranslate||(h.support.transforms3d?h.wrapper.transform(\"translate3d(\"+r+\"px, \"+o+\"px, \"+a+\"px)\"):h.wrapper.transform(\"translate(\"+r+\"px, \"+o+\"px)\")),h.translate=h.isHorizontal()?r:o;var s=h.maxTranslate()-h.minTranslate();i=0===s?0:(t-h.minTranslate())/s,i!==h.progress&&h.updateProgress(t),e&&h.updateActiveIndex(),\"slide\"!==h.params.effect&&h.effects[h.params.effect]&&h.effects[h.params.effect].setTranslate(h.translate),h.params.parallax&&h.parallax&&h.parallax.setTranslate(h.translate),h.params.scrollbar&&h.scrollbar&&h.scrollbar.setTranslate(h.translate),h.params.control&&h.controller&&h.controller.setTranslate(h.translate,n),h.emit(\"onSetTranslate\",h,h.translate)},h.getTranslate=function(t,e){var n,i,r,o;return\"undefined\"===typeof e&&(e=\"x\"),h.params.virtualTranslate?h.rtl?-h.translate:h.translate:(r=window.getComputedStyle(t,null),window.WebKitCSSMatrix?(i=r.transform||r.webkitTransform,i.split(\",\").length>6&&(i=i.split(\", \").map(function(t){return t.replace(\",\",\".\")}).join(\", \")),o=new window.WebKitCSSMatrix(\"none\"==